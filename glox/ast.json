{
	"Expr": {
		"Assignment": [
			"Name lexer.Token",
			"Value Expr"
		],
		"Binary": [
			"Left Expr",
			"Operator lexer.Token",
			"Right Expr"
		],
		"Call": [
			"Callee Expr",
			"ClosingParen lexer.Token",
			"Args []Expr"
		],
		"Get": [
			"Object Expr",
			"Name lexer.Token"
		],
		"Logical": [
			"Left Expr",
			"Operator lexer.Token",
			"Right Expr"
		],
		"Set": [
			"Object Expr",
			"Name lexer.Token",
			"Value Expr"
		],
		"Unary": [
			"Operator lexer.Token",
			"Right Expr"
		],
		"Grouping": ["Expression Expr"],
		"Literal": ["Value any"],
		"Variable": ["Name lexer.Token"]
	},
	"Stmt": {
		"Block": ["Statements []Stmt"],
		"Break": ["Continue bool"],
		"Class": ["Name lexer.Token", "Methods []*Function"],
		"Expression": ["Expression Expr"],
		"Function": [
			"Name lexer.Token",
			"Params []lexer.Token",
			"Body []Stmt"
		],
		"If": [
			"Condition Expr",
			"ThenBranch Stmt",
			"ElseBranch Stmt"
		],
		"Print": ["Expression Expr"],
		"Return": ["Expression Expr", "Token lexer.Token"],
		"Var": [
			"Name lexer.Token",
			"Initializer Expr"
		],
		"While": [
			"Condition Expr",
			"Do Stmt"
		]
	}
}
